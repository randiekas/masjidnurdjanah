{
    "detil":  {
        "id": 91,
        "surat_name": "Asy-Syams",
        "surat_text": " \u0627\u0644\u0634\u0645\u0633",
        "surat_terjemahan": "Matahari",
        "count_ayat": 15,
        "tersedia": true
    },
    "ayat": [
        {
            "aya_id": 6044,
            "aya_number": 1,
            "aya_text": "\u0648\u064e\u0627\u0644\u0634\u0651\u064e\u0645\u0652\u0633\u0650 \u0648\u064e\u0636\u064f\u062d\u0670\u0649\u0647\u064e\u0627\u06d6",
            "sura_id": 91,
            "juz_id": 30,
            "page_number": 595,
            "translation_aya_text": "Demi matahari dan sinarnya pada pagi hari,"
        }, {
            "aya_id": 6045,
            "aya_number": 2,
            "aya_text": "\u0648\u064e\u0627\u0644\u0652\u0642\u064e\u0645\u064e\u0631\u0650 \u0627\u0650\u0630\u064e\u0627 \u062a\u064e\u0644\u0670\u0649\u0647\u064e\u0627\u06d6",
            "sura_id": 91,
            "juz_id": 30,
            "page_number": 595,
            "translation_aya_text": "demi bulan apabila mengiringinya,"
        }, {
            "aya_id": 6046,
            "aya_number": 3,
            "aya_text": "\u0648\u064e\u0627\u0644\u0646\u0651\u064e\u0647\u064e\u0627\u0631\u0650 \u0627\u0650\u0630\u064e\u0627 \u062c\u064e\u0644\u0651\u0670\u0649\u0647\u064e\u0627\u06d6",
            "sura_id": 91,
            "juz_id": 30,
            "page_number": 595,
            "translation_aya_text": "demi siang apabila menampakkannya,"
        }, {
            "aya_id": 6047,
            "aya_number": 4,
            "aya_text": "\u0648\u064e\u0627\u0644\u0651\u064e\u064a\u0652\u0644\u0650 \u0627\u0650\u0630\u064e\u0627 \u064a\u064e\u063a\u0652\u0634\u0670\u0649\u0647\u064e\u0627\u06d6",
            "sura_id": 91,
            "juz_id": 30,
            "page_number": 595,
            "translation_aya_text": "demi malam apabila menutupinya (gelap gulita),"
        }, {
            "aya_id": 6048,
            "aya_number": 5,
            "aya_text": "\u0648\u064e\u0627\u0644\u0633\u0651\u064e\u0645\u064e\u0627\u06e4\u0621\u0650 \u0648\u064e\u0645\u064e\u0627 \u0628\u064e\u0646\u0670\u0649\u0647\u064e\u0627\u06d6",
            "sura_id": 91,
            "juz_id": 30,
            "page_number": 595,
            "translation_aya_text": "demi langit serta pembinaannya (yang menakjubkan),"
        }, {
            "aya_id": 6049,
            "aya_number": 6,
            "aya_text": "\u0648\u064e\u0627\u0644\u0652\u0627\u064e\u0631\u0652\u0636\u0650 \u0648\u064e\u0645\u064e\u0627 \u0637\u064e\u062d\u0670\u0649\u0647\u064e\u0627\u06d6",
            "sura_id": 91,
            "juz_id": 30,
            "page_number": 595,
            "translation_aya_text": "demi bumi serta penghamparannya,"
        }, {
            "aya_id": 6050,
            "aya_number": 7,
            "aya_text": "\u0648\u064e\u0646\u064e\u0641\u0652\u0633\u064d \u0648\u0651\u064e\u0645\u064e\u0627 \u0633\u064e\u0648\u0651\u0670\u0649\u0647\u064e\u0627\u06d6",
            "sura_id": 91,
            "juz_id": 30,
            "page_number": 595,
            "translation_aya_text": "demi jiwa serta penyempurnaan (ciptaan)nya,"
        }, {
            "aya_id": 6051,
            "aya_number": 8,
            "aya_text": "\u0641\u064e\u0627\u064e\u0644\u0652\u0647\u064e\u0645\u064e\u0647\u064e\u0627 \u0641\u064f\u062c\u064f\u0648\u0652\u0631\u064e\u0647\u064e\u0627 \u0648\u064e\u062a\u064e\u0642\u0652\u0648\u0670\u0649\u0647\u064e\u0627\u06d6",
            "sura_id": 91,
            "juz_id": 30,
            "page_number": 595,
            "translation_aya_text": "maka Dia mengilhamkan kepadanya (jalan) kejahatan dan ketakwaannya,"
        }, {
            "aya_id": 6052,
            "aya_number": 9,
            "aya_text": "\u0642\u064e\u062f\u0652 \u0627\u064e\u0641\u0652\u0644\u064e\u062d\u064e \u0645\u064e\u0646\u0652 \u0632\u064e\u0643\u0651\u0670\u0649\u0647\u064e\u0627\u06d6",
            "sura_id": 91,
            "juz_id": 30,
            "page_number": 595,
            "translation_aya_text": "sungguh beruntung orang yang menyucikannya (jiwa itu),"
        }, {
            "aya_id": 6053,
            "aya_number": 10,
            "aya_text": "\u0648\u064e\u0642\u064e\u062f\u0652 \u062e\u064e\u0627\u0628\u064e \u0645\u064e\u0646\u0652 \u062f\u064e\u0633\u0651\u0670\u0649\u0647\u064e\u0627\u06d7",
            "sura_id": 91,
            "juz_id": 30,
            "page_number": 595,
            "translation_aya_text": "dan sungguh rugi orang yang mengotorinya."
        }
    ],
